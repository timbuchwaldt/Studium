
public class a51 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int a,b;
		System.out.println("Bitte Zahl 1 eingeben");
		a = SimpleIO.getInt();
		System.out.println("Bitte Zahl 2 eingeben");
		b = SimpleIO.getInt();
		if(a == 0){
			System.out.println(b);
		}else{
			while (b!=0){
				if(a>b){
					a= a-b;
				}else{
					b = b-a;
				}
			}
		}
	}

}
